{
  "name": "ember-cli-intl-shake",
  "version": "1.0.1",
  "description": "Shake intl files using app literals",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --max-warnings 0 ./*.js lib node-tests config tests",
    "start": "ember serve",
    "test": "nyc mocha --recursive node-tests",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 85 --functions 85 --branches 85 --lines 85",
    "commit": "git-cz",
    "posttest": "npm run report-coverage && npm run check-coverage && npm run validate-coverage",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "semantic-release": "semantic-release",
    "validate-coverage": "test \"$(cat coverage/coverage-summary.json | json total.lines.total)\" -gt 0"
  },
  "dependencies": {
    "@glimmer/syntax": "^0.44.0",
    "acorn": "^7.1.0",
    "acorn-walk": "^7.0.0",
    "async": "^3.1.0",
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^3.0.2",
    "broccoli-persistent-filter": "^2.3.1",
    "broccoli-plugin": "^3.0.0",
    "broccoli-source": "^3.0.0",
    "debug": "^4.1.1",
    "ember-cli-babel": "^7.13.1",
    "estree-walker": "^0.9.0",
    "fs-extra": "^8.1.0",
    "fs-tree-diff": "^2.0.1",
    "hash-for-dep": "^1.5.1",
    "json-to-ast": "^2.1.0",
    "strip-bom": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@ember/optional-features": "^1.1.0",
    "@semantic-release/changelog": "^3.0.5",
    "@semantic-release/git": "^7.0.17",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-test-helper": "^2.0.0",
    "chai": "^4.2.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eager-engine": "file:./tests/dummy/lib/eager-engine",
    "ember-cli": "^3.13.1",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-htmlbars": "^4.0.8",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-engines": "^0.8.2",
    "ember-export-application-global": "^2.0.0",
    "ember-intl": "^4.2.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "^3.13.3",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.2.1",
    "eslint": "^6.6.0",
    "eslint-config-bbva": "^2.0.4",
    "eslint-plugin-ember": "^7.2.0",
    "eslint-plugin-node": "^10.0.0",
    "husky": "^3.0.9",
    "istanbul": "^0.4.5",
    "json": "^9.0.6",
    "lazy-engine": "file:./tests/dummy/lib/lazy-engine",
    "loader.js": "^4.7.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "qunit-dom": "^0.9.0",
    "rimraf": "^3.0.0",
    "semantic-release": "^15.13.28",
    "sinon": "^7.5.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "broccoli-asset-rev"
    ],
    "paths": [
      "tests/dummy/lib/dummy-addon"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
